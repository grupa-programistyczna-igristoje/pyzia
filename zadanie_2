#include <iostream>

int main()
{
	int punkty;

	std::cout << "Podaj liczbe swoich punktow\n";
	std::cin >> punkty;

	if (100 > punkty && punkty > 90) {													//ocena 6 

		std::cout << "Brawo! Dostales 6, oby tak dalej!" << std::endl;
		}
		if (punkty < 89 && punkty > 80) {													//ocena 5 

		std::cout << "Dobrze! Widac, ze sie postarales" << std::endl; 
		}
		if (punkty < 79 && punkty > 70) {													//ocena 4

			std::cout << "Niezle, ale stac cie na lepiej" << std::endl;
		}
		if (punkty < 69 && punkty > 60) {													//ocena 3 

			std::cout << "Wymagana wiedza, nic wiecej" << std::endl; 
		}
		if (punkty < 59 && punkty > 50) {													//ocena 2

			std::cout << "Ledwo zdales, bez wiedzy nic nie osiagniesz" << std::endl; 
		}
		if (punkty < 49 && punkty > 0) {													//ocena 1 

			std::cout << "Nie zdales, jak Oli Dziminski XDDD" << std::endl; 
		}
						
}
